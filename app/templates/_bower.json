{
  "name": "<%= _.slugify(props.project) %>",
  "version": "0.0.0",
  "dependencies": {<% if (props.router && includedRouter === 'director') { %>
    "director": "~1.2.2",<% } if (includejQuery) { %>
    "jQuery": "~1.11.1",<% } if (includeModernizr) { %>
    "modernizr": "~2.8.2",<% } if (includeNormalize) { if (!includeSass) { %>
    "normalize-css": "~3.0.1",<% } else { %>
    "normalize-scss": "~3.0.1",<% }} if (props.router && includedRouter == 'page') { %>
    "page": "~1.3.7",<% } if (loadMethod !== 'browserify') { %>
    "ractive": "~0.5.5"<% } if (loadMethod === 'AMD') { %>,
    "requirejs-ractive": "~0.1.5",
    "requirejs": "~2.1.14"<% } if (props.router && includedRouter === 'router.js') { %>,
    "router.js": "^0.6.2"<% } %>
  },
  "devDependencies": {<% if (testFramework === 'mocha') {  %>
    "chai": "^1.9.1",
    "mocha": "^1.21.4"<% } %>
  }<% if (props.router) { %>,
  "overrides": {
    "<%= includedRouter %>": {
      "main": "<%= includedRouterMainPath %>"
    }
  }<% } %>
}
